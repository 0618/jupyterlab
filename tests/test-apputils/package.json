    "@lumino/widgets": "^1.9.4",
    "chai": "^4.2.0",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "react": "~16.9.0",
    "simulate-event": "~1.4.0",
    "ts-jest": "^24.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
