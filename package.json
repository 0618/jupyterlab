{
  "name": "@jupyterlab/debugger",
  "version": "0.1.0",
  "description": "JupyterLab - Debugger Extension",
  "homepage": "https://github.com/jupyterlab/debugger",
  "bugs": {
    "url": "https://github.com/jupyterlab/debugger/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/debugger.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js.map",
    "lib/**/*.js",
    "schema/*.json",
    "style/**/*.css",
    "style/**/*.svg"
  ],
  "sideEffects": [
    "style/**/*.css"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib",
    "docs": "typedoc --options tdoptions.json --theme ../../typedoc-theme src",
    "prepublishOnly": "npm run build",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/apputils": "^1.0.0",
    "@jupyterlab/codeeditor": "^1.0.0",
    "@jupyterlab/console": "^1.0.2",
    "@jupyterlab/coreutils": "^3.0.0",
    "@jupyterlab/fileeditor": "^1.0.2",
    "@jupyterlab/launcher": "^1.0.0",
    "@jupyterlab/notebook": "^1.0.0",
    "@phosphor/coreutils": "^1.3.1",
    "@phosphor/disposable": "^1.2.0",
    "@phosphor/widgets": "^1.8.0"
  },
  "devDependencies": {
    "prettier": "^1.18.2",
    "rimraf": "~2.6.2",
    "tslint": "^5.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typedoc": "^0.15.0",
    "typescript": "~3.5.1"
  },
  "publishConfig": {
    "access": "private"
  },
  "private": true,
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
