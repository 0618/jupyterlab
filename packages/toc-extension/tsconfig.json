{
  "compilerOptions": {
    "declaration": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "lib": [
      "DOM",
      "ES6"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES6",
    "outDir": "lib",
    "rootDir": "src",
    "jsx": "react"
  },
  "include": [
    "src/**/*"
  ],
  "references": [
    {
      "path": "../application"
    },
    {
      "path": "../apputils"
    },
    {
      "path": "../docmanager"
    },
    {
      "path": "../fileeditor"
    },
    {
      "path": "../markdownviewer"
    },
    {
      "path": "../notebook"
    },
    {
      "path": "../rendermime"
    },
    {
      "path": "../toc"
    },
    {
      "path": "../settingregistry"
    }
  ],
  "extends": "../../tsconfigbase"
}
