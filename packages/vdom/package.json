    "@jupyterlab/services": "^6.0.0-beta.0",
    "@lumino/coreutils": "^1.5.3",
    "@lumino/messaging": "^1.4.3",
    "@lumino/widgets": "^1.14.0",
    "@nteract/transform-vdom": "^4.0.16-alpha.0",
    "react": "~16.13.1",
    "react-dom": "~16.13.1"
  },
  "devDependencies": {
    "@types/react": "~16.9.48",
    "@types/react-dom": "~16.9.8",
    "rimraf": "~3.0.0",
