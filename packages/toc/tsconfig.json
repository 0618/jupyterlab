{
  "compilerOptions": {
    "declaration": true,
    "composite": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "lib": ["DOM", "ES6"],
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES6",
    "outDir": "lib",
    "rootDir": "src",
    "jsx": "react"
  },
  "include": ["src/**/*"],
  "references": [
    {
      "path": "../apputils"
    },
    {
      "path": "../cells"
    },
    {
      "path": "../coreutils"
    },
    {
      "path": "../docmanager"
    },
    {
      "path": "../docregistry"
    },
    {
      "path": "../fileeditor"
    },
    {
      "path": "../markdownviewer"
    },
    {
      "path": "../notebook"
    },
    {
      "path": "../rendermime"
    },
    {
      "path": "../settingregistry"
    }
  ],
  "extends": "../../tsconfigbase"
}
